{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "asana.png",
      "default_popup": "popup.html",
      "default_title": "Asana Task Time"
   },
   "description": "Fully Automated Time Tracking on Asana",
   "icons": {
      "128": "asana.png"
   },
   "manifest_version": 2,
   "name": "Time Tracker",
   "permissions": [ 
      "tabs" , 
      "history"
   ],
   "version": "1.0"
}
